<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="279.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane>
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="685.0" minWidth="10.0" prefWidth="602.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints maxHeight="395.0" minHeight="10.0" prefHeight="384.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="52.0" minHeight="10.0" prefHeight="46.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <TableView fx:id="tableId" prefHeight="400.0" prefWidth="601.0">
              <columns>
                <TableColumn fx:id="AirlineNameId" prefWidth="150.0" text="Airline" />
                <TableColumn fx:id="FlightId" prefWidth="120.0" text="Flight id" />
                  <TableColumn fx:id="DepartureId" prefWidth="109.0" text="Departure" />
                  <TableColumn fx:id="ArrivalId" prefWidth="128.0" text="Arrival" />
                  <TableColumn fx:id="DateId" prefWidth="83.0" text="Date" />
              </columns>
            </TableView>
            <Button fx:id="ReservationButton" mnemonicParsing="false" onAction="#ReservationAction" text="Reservation" GridPane.rowIndex="1">
               <GridPane.margin>
                  <Insets left="500.0" />
               </GridPane.margin>
            </Button>
         </children>
      </GridPane>
   </children>
</AnchorPane>
